{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "artesaos/seotools": "^1.2",
        "bacon/bacon-qr-code": "^2.0",
        "coinpaymentsnet/coinpayments-php": "^1.4",
        "danog/madelineproto": "^8.0",
        "doocacommerce/paghiper-php-sdk": "^1.2",
        "fruitcake/laravel-cors": "^3.0",
        "grpc/grpc": "^1.57",
        "guzzlehttp/guzzle": "^7.0.1",
        "hansschouten/laravel-pagebuilder": "^0.31",
        "intervention/image": "^2.7",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel-lang/lang": "^14.7",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.0",
        "laravel/octane": "^1.5",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "mercadopago/dx-php": "^2.5",
        "mews/purifier": "^3.3",
        "mollie/laravel-mollie": "^2.20",
        "nexmo/client": "^2.4",
        "nowpayments/nowpayments-api-php": "dev-main",
        "pragmarx/google2fa-laravel": "^2.1",
        "predis/predis": "^3.3",
        "psr/log": "^3.0",
        "sheaf/cli": "^1.3",
        "sinergi/browser-detector": "^6.1",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-cookie-consent": "^3.0",
        "spatie/laravel-health": "^1.0",
        "spatie/laravel-permission": "^5.9",
        "stevebauman/location": "^6.6",
        "stripe/stripe-php": "^10.11",
        "victorybiz/laravel-crypto-payment-gateway": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "imanghafoori/laravel-microscope": "^1.0",
        "knuckleswtf/scribe": "^5.6",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Addons\\MultiChannelSignalAddon\\": "addons/multi-channel-signal-addon/",
            "Addons\\MultiChannelSignalAddon\\App\\": "addons/multi-channel-signal-addon/app/",
            "Addons\\TradingBotSignalAddon\\": "addons/trading-bot-signal-addon/",
            "Addons\\TradingBotSignalAddon\\App\\": "addons/trading-bot-signal-addon/app/",
            "Addons\\OpenRouterIntegration\\": "addons/openrouter-integration-addon/",
            "Addons\\OpenRouterIntegration\\App\\": "addons/openrouter-integration-addon/app/",
            "Addons\\AiConnectionAddon\\": "addons/ai-connection-addon/",
            "Addons\\AiConnectionAddon\\App\\": "addons/ai-connection-addon/App/",
            "Addons\\TradingManagement\\": "addons/trading-management-addon/",
            "Addons\\TradingManagement\\Modules\\": "addons/trading-management-addon/Modules/",
            "Addons\\PageBuilderAddon\\": "addons/page-builder-addon/",
            "Addons\\PageBuilderAddon\\App\\": "addons/page-builder-addon/app/",
            "Addons\\AlgoExpertPlus\\": "addons/algoexpert-plus-addon/",
            "Addons\\AlgoExpertPlus\\App\\": "addons/algoexpert-plus-addon/app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "app/Helpers/Helper.php",
            "app/Utility/FormBuilder.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "docs:generate": "php artisan scribe:generate",
        "docs:openapi": "php artisan scribe:generate",
        "docs:refresh": [
            "@docs:generate"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "hansschouten/laravel-pagebuilder"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
